{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": 0,
    "import/named": 0,
    "import/prefer-default-export": "off",
    "arrow-body-style": "off",
    "no-shadow": 0
  }
}
